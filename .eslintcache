    }\r\n  };\r\n\r\n  const onChange = ({ target }) => setMessage(target.value);\r\n\r\n  const sendMessage = () => {\r\n    if (message.length > 0 && sltdContacts.length > 0) {\r\n      sltdContacts.map((id) => {\r\n        dispatch({\r\n          type: types.updateUnreaded,\r\n          payload: { id, value: 1 },\r\n        });\r\n\r\n        dispatch({\r\n          type: types.addMultiMessages,\r\n          payload: {\r\n            id,\r\n            message: { origin: \"from\", message },\r\n          },\r\n        });\r\n\r\n        if (activeContact._id === id) {\r\n          dispatch({\r\n            type: types.addMessage,\r\n            payload: { origin: \"from\", message },\r\n          });\r\n\r\n          setTimeout(() => {\r\n            scrollToBottomAnimated(\"messages\");\r\n          }, 100);\r\n        }\r\n      });\r\n\r\n      addToast(`Message '${message}' was sended successfully.`, {\r\n        appearance: \"info\",\r\n      });\r\n\r\n      setMessage(\"\");\r\n      setSltdContacts([]);\r\n      closeTooltip();\r\n    } else {\r\n      addToast(\r\n        `You need to write something and select a contact if you want to send a message.`,\r\n        {\r\n          appearance: \"error\",\r\n        }\r\n      );\r\n    }\r\n  };\r\n\r\n  return (\r\n    <Popup ref={ref} trigger={trigger} modal>\r\n      {(close) => (\r\n        <div className=\"popup__send-message\">\r\n          <h4>Send Message</h4>\r\n\r\n          <textarea\r\n            onChange={onChange}\r\n            value={message}\r\n            className=\"b-shadow\"\r\n            placeholder=\"Write your message...\"\r\n          ></textarea>\r\n\r\n          <div className=\"popup__contacts\">\r\n            <input\r\n              onChange={searchContact}\r\n              value={search}\r\n              type=\"text\"\r\n              placeholder=\"Search...\"\r\n            />\r\n            <ul className=\"scroll\">\r\n              {contacts.map(\r\n                (contact, idx) =>\r\n                  contact.name.toLowerCase().includes(search.toLowerCase()) && (\r\n                    <li\r\n                      onClick={() =>\r\n                        selected(sltdContacts, setSltdContacts, contact._id)\r\n                      }\r\n                      className={`b-shadow ${\r\n                        selectedClass(contact._id, sltdContacts) &&\r\n                        \"popup__send-message-selected\"\r\n                      }`}\r\n                      key={idx}\r\n                    >\r\n                      <img\r\n                        className=\"circle-img\"\r\n                        src=\"https://picsum.photos/200/200\"\r\n                      />\r\n                      <span>{contact.name}</span>\r\n                    </li>\r\n                  )\r\n              )}\r\n            </ul>\r\n          </div>\r\n\r\n          <div className=\"popup__actions\">\r\n            <button onClick={sendMessage}>Confirm</button>\r\n            <button onClick={close}>Close</button>\r\n          </div>\r\n        </div>\r\n      )}\r\n    </Popup>\r\n  );\r\n};\r\n","C:\\Users\\jesus\\Documents\\Work\\Amor\\evitiu-sms\\src\\components\\popup\\CreateTeam.js",[],"C:\\Users\\jesus\\Documents\\Work\\Amor\\evitiu-sms\\src\\components\\popup\\AddTeamContact.js",["153","154"],"C:\\Users\\jesus\\Documents\\Work\\Amor\\evitiu-sms\\src\\components\\popup\\RemoveTeamContact.js",["155","156"],{"ruleId":"157","replacedBy":"158"},{"ruleId":"159","replacedBy":"160"},{"ruleId":"161","severity":1,"message":"162","line":19,"column":22,"nodeType":"163","messageId":"164","endLine":19,"endColumn":24},{"ruleId":"165","severity":1,"message":"166","line":1,"column":29,"nodeType":"167","messageId":"168","endLine":1,"endColumn":38},{"ruleId":"165","severity":1,"message":"169","line":1,"column":48,"nodeType":"167","messageId":"168","endLine":1,"endColumn":56},{"ruleId":"165","severity":1,"message":"170","line":8,"column":26,"nodeType":"167","m